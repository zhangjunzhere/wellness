<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    style="@style/profilebg"

    >
    <com.asus.wellness.view.MyTextView
        android:id="@+id/tv_page_title"
        android:layout_width="match_parent"
        android:layout_height="@dimen/profile_title_height"
        android:text="@string/title_workout_insert_data"
        style="@style/coach_title"
        />

    <View
        android:id="@+id/line"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="1dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:layout_marginLeft="@dimen/margin_left">
        <android.support.wearable.view.WearableListView
            android:id="@+id/list0"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
          />

        <include layout="@layout/pni_coach_fragment_workout_data_seperators"
            android:id="@+id/ll_seperator0"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"/>

        <android.support.wearable.view.WearableListView
            android:id="@+id/list1"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            />

        <include layout="@layout/pni_coach_fragment_workout_data_seperators"
            android:id="@+id/ll_seperator1"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"/>

    </LinearLayout>

</LinearLayout>

<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/dailyanim_layout_width"
    android:layout_height="match_parent"
    android:layout_gravity="center_horizontal"
    android:visibility="visible" >

    <FrameLayout
        android:id="@+id/R_group"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <FrameLayout
            android:id="@+id/r_frame5"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="right" >

            <FrameLayout
                android:id="@+id/R5_leave"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:layout_marginRight="@dimen/dailyanim_R5_leave_marginRight"
                android:layout_marginTop="@dimen/dailyanim_R5_leave_marginTop" >

                <ImageView
                    android:id="@+id/orangeleaves04"
                    android:layout_width="@dimen/dailyanim_orangeleaves04_width"
                    android:layout_height="@dimen/dailyanim_orangeleaves04_height"
                    android:src="@drawable/asus_wellness_btn_orange_leaves_4" />

                <ImageView
                    android:id="@+id/orangeleaves5"
                    android:layout_width="@dimen/dailyanim_orangeleaves5_width"
                    android:layout_height="@dimen/dailyanim_orangeleaves5_height"
                    android:src="@drawable/asus_wellness_btn_orange_leaves_5" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/R5_circle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:layout_marginRight="@dimen/dailyanim_R5_circle_marginRight"
                android:layout_marginTop="@dimen/dailyanim_R5_circle_marginTop" >

                <ImageView
                    android:id="@+id/R5_slave_cirlce"
                    android:layout_width="@dimen/dailyanim_R5_slave_cirlce_width"
                    android:layout_height="@dimen/dailyanim_R5_slave_cirlce_height"
                    android:src="@drawable/r_circle4" />

                <ImageView
                    android:id="@+id/R5_master_cirlce"
                    android:layout_width="@dimen/dailyanim_R5_master_cirlce_width"
                    android:layout_height="@dimen/dailyanim_R5_master_cirlce_height"
                    android:src="@drawable/r_circle5" />
            </FrameLayout>
        </FrameLayout>

        <FrameLayout
            android:id="@+id/r_frame4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="right" >

            <FrameLayout
                android:id="@+id/R4_leave"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:layout_marginRight="@dimen/dailyanim_R4_leave_marginRight"
                android:layout_marginTop="@dimen/dailyanim_R4_leave_marginTop" >

                <ImageView
                    android:id="@+id/orangeleaves03"
                    android:layout_width="@dimen/dailyanim_orangeleaves03_width"
                    android:layout_height="@dimen/dailyanim_orangeleaves03_height"
                    android:layout_gravity="right"
                    android:src="@drawable/asus_wellness_btn_orange_leaves_3" />

                <ImageView
                    android:id="@+id/orangeleaves4"
                    android:layout_width="@dimen/dailyanim_orangeleaves4_width"
                    android:layout_height="@dimen/dailyanim_orangeleaves4_height"
                    android:src="@drawable/asus_wellness_btn_orange_leaves_4" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/R4_circle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:layout_marginRight="@dimen/dailyanim_R4_circle_marginRight"
                android:layout_marginTop="@dimen/dailyanim_R4_circle_marginTop" >

                <ImageView
                    android:id="@+id/R4_slave_cirlce"
                    android:layout_width="@dimen/dailyanim_R4_slave_cirlce_width"
                    android:layout_height="@dimen/dailyanim_R4_slave_cirlce_height"
                    android:src="@drawable/r_circle3" />

                <ImageView
                    android:id="@+id/R4_master_cirlce"
                    android:layout_width="@dimen/dailyanim_R4_master_cirlce_width"
                    android:layout_height="@dimen/dailyanim_R4_master_cirlce_height"
                    android:src="@drawable/r_circle4" />
            </FrameLayout>
        </FrameLayout>

        <FrameLayout
            android:id="@+id/r_frame3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="right" >

            <FrameLayout
                android:id="@+id/R3_leave"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:layout_marginRight="@dimen/dailyanim_R3_leave_marginRight"
                android:layout_marginTop="@dimen/dailyanim_R3_leave_marginTop" >

                <ImageView
                    android:id="@+id/orangeleaves02"
                    android:layout_width="@dimen/dailyanim_orangeleaves02_width"
                    android:layout_height="@dimen/dailyanim_orangeleaves02_height"
                    android:layout_gravity="right"
                    android:src="@drawable/asus_wellness_btn_orange_leaves_2" />

                <ImageView
                    android:id="@+id/orangeleaves3"
                    android:layout_width="@dimen/dailyanim_orangeleaves3_width"
                    android:layout_height="@dimen/dailyanim_orangeleaves3_height"
                    android:src="@drawable/asus_wellness_btn_orange_leaves_3" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/R3_circle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:layout_marginRight="@dimen/dailyanim_R3_circle_marginRight"
                android:layout_marginTop="@dimen/dailyanim_R3_circle_marginTop" >

                <ImageView
                    android:id="@+id/R3_slave_cirlce"
                    android:layout_width="@dimen/dailyanim_R3_slave_cirlce_width"
                    android:layout_height="@dimen/dailyanim_R3_slave_cirlce_height"
                    android:src="@drawable/r_circle2" />

                <ImageView
                    android:id="@+id/R3_master_cirlce"
                    android:layout_width="@dimen/dailyanim_R3_master_cirlce_width"
                    android:layout_height="@dimen/dailyanim_R3_master_cirlce_height"
                    android:src="@drawable/r_circle3" />
            </FrameLayout>
        </FrameLayout>

        <FrameLayout
            android:id="@+id/r_frame2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="right" >

            <FrameLayout
                android:id="@+id/R2_leave"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:layout_marginRight="@dimen/dailyanim_R2_leave_marginRight"
                android:layout_marginTop="@dimen/dailyanim_R2_leave_marginTop" >

                <ImageView
                    android:id="@+id/orangeleaves01"
                    android:layout_width="@dimen/dailyanim_orangeleaves01_width"
                    android:layout_height="@dimen/dailyanim_orangeleaves01_height"
                    android:src="@drawable/asus_wellness_btn_orange_leaves_1" />

                <ImageView
                    android:id="@+id/orangeleaves2"
                    android:layout_width="@dimen/dailyanim_orangeleaves2_width"
                    android:layout_height="@dimen/dailyanim_orangeleaves2_height"
                    android:src="@drawable/asus_wellness_btn_orange_leaves_2" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/R2_circle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:layout_marginRight="@dimen/dailyanim_R2_circle_marginRight"
                android:layout_marginTop="@dimen/dailyanim_R2_circle_marginTop" >

                <ImageView
                    android:id="@+id/R2_slave_cirlce"

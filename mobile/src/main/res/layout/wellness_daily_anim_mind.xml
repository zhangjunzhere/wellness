                    android:layout_height="@dimen/dailyanim_R2_master_cirlce_height"
                    android:src="@drawable/r_circle2" />

                <TextView
                    android:id="@+id/r2_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/dailyanim_r2_txt_marginTop"
                    android:text=""
                    android:textColor="@color/fontColor1"
                    android:textSize="@dimen/dailyanim_r2_txt_textsize" />
            </FrameLayout>
        </FrameLayout>

        <FrameLayout
            android:id="@+id/r_frame1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="right" >

            <FrameLayout
                android:id="@+id/R1_leave"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:layout_marginRight="@dimen/dailyanim_R1_leave_marginRight"
                android:layout_marginTop="@dimen/dailyanim_R1_leave_marginTop" >

                <ImageView
                    android:id="@+id/orangeleaves05"
                    android:layout_width="@dimen/dailyanim_orangeleaves05_width"
                    android:layout_height="@dimen/dailyanim_orangeleaves05_height"
                    android:src="@drawable/asus_wellness_btn_orange_leaves_2" />

                <ImageView
                    android:id="@+id/orangeleaves1"
                    android:layout_width="@dimen/dailyanim_orangeleaves1_width"
                    android:layout_height="@dimen/dailyanim_orangeleaves1_height"
                    android:src="@drawable/asus_wellness_btn_orange_leaves_1" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/R1_circle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:layout_marginRight="@dimen/dailyanim_R1_circle_marginRight"
                android:layout_marginTop="@dimen/dailyanim_R1_circle_marginTop" >

                <ImageView
                    android:id="@+id/tips_R_circle_standard"
                    android:layout_width="@dimen/dailyanim_R1_slave_cirlce_width"
                    android:layout_height="@dimen/dailyanim_R1_slave_cirlce_height"
                    android:src="@drawable/r_circle2"
                    android:visibility="invisible"/>

                <ImageView
                    android:id="@+id/R1_slave_cirlce"
                    android:layout_width="@dimen/dailyanim_R1_slave_cirlce_width"
                    android:layout_height="@dimen/dailyanim_R1_slave_cirlce_height"
                    android:src="@drawable/r_circle2" />

                <ImageView
                    android:id="@+id/R1_master_cirlce"
                    android:layout_width="@dimen/dailyanim_R1_master_cirlce_width"
                    android:layout_height="@dimen/dailyanim_R1_master_cirlce_height"
                    android:src="@drawable/r_circle1" />

                <TextView
                    android:id="@+id/r1_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/dailyanim_r1_txt_marginTop"
                    android:text=""
                    android:textColor="@color/fontColor1"
                    android:textSize="@dimen/dailyanim_r1_txt_textsize" />

                <TextView
                    android:id="@+id/Average"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="@dimen/dailyanim_average_marginTop"
                    android:text="@string/text_average"
                    android:textColor="@color/fontColor1"
                    android:textSize="@dimen/dailyanim_average_textsize" />
            </FrameLayout>
        </FrameLayout>

        <ImageView
            android:id="@+id/r_large"
            android:layout_width="@dimen/dailyanim_r_large_width"
            android:layout_height="@dimen/dailyanim_r_large_height"
            android:layout_gravity="right"
            android:layout_marginRight="@dimen/dailyanim_r_large_marginRight"
            android:layout_marginTop="@dimen/dailyanim_r_large_marginTop"
            android:src="@drawable/r_circle1" />
    </FrameLayout>


    <TextView
        android:id="@+id/Energy_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right"
        android:layout_marginRight="@dimen/dailyanim_Energy_txt_marginRight"
        android:layout_marginTop="@dimen/dailyanim_Energy_txt_marginTop"
        android:gravity="center"
        android:textAlignment="center"
        android:text="@string/text_energy"
        android:textColor="#db213f"
        android:textSize="@dimen/dailyanim_Energy_txt_textsize"
        android:textStyle="bold"
        android:width="@dimen/dailyanim_Energy_txt_width" />

    <ImageView
        android:id="@+id/energy_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right"
        android:layout_marginRight="@dimen/dailyanim_energy_btn_marginRight"
        android:layout_marginTop="@dimen/dailyanim_energy_btn_marginTop"
        android:alpha="0"
        android:src="@drawable/r_circle1" />

</FrameLayout>